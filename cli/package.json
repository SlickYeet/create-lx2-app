{
  "name": "create-tnt-stack",
  "version": "0.2.5",
  "description": "Create web application with the TNT-Powered stack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/slickyeet/create-tnt-stack",
    "directory": "cli"
  },
  "keywords": [
    "create-tnt-stack",
    "tnt-stack",
    "typescript",
    "next.js",
    "tailwind"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "create-tnt-stack": "./dist/index.js"
  },
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsup",
    "start": "node dist/index.js",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --write",
    "typecheck": "tsc",
    "clean": "rm -rf dist .turbo node_modules",
    "release": "bun run clean build typecheck && npm version patch && npm publish --access public"
  },
  "dependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@inquirer/prompts": "^7.3.3",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "gradient-string": "^3.0.0",
    "ora": "^8.2.0",
    "sort-package-json": "^3.0.0"
  },
  "devDependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@prisma/client": "^6.4.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@types/fs-extra": "^11.0.4",
    "next": "^15.2.1",
    "next-auth": "^4.24.11",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.11",
    "tsup": "^8.4.0",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2",
    "zod": "^3.24.2"
  }
}
